	"dependencies": {
		"apollo-datasource-rest": "^0.6.9",
		"apollo-server": "^2.25.3",
		"graphql": "^14.5.8"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
